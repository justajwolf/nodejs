{
  "name": "@justajwolf/body-parser-xml",
  "version": "1.0.0",
  "description": "This is nodejs body parser xml middleware.",
  "keywords": [
    "express",
    "xml",
    "parser",
    "body-parser"
  ],
  "author": "justajwolf",
  "homepage": "https://github.com/justajwolf/nodejs#readme",
  "license": "MIT",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "src": "src",
    "test": "test"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justajwolf/nodejs.git"
  },
  "scripts": {
    "build": "gulp",
    "lint-staged": "lint-staged",
    "lint": "./node_modules/.bin/eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --detectOpenHandles 2>&1"
  },
  "bugs": {
    "url": "https://github.com/justajwolf/nodejs/issues"
  },
  "dependencies": {
    "bytes": "^3.1.1",
    "type-is": "^1.6.18",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/bytes": "^3.1.1",
    "@types/express": "^4.17.13",
    "@types/gulp": "^4.0.9",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.5",
    "@types/supertest": "^2.0.11",
    "@types/type-is": "^1.6.3",
    "@types/xml2js": "^0.4.9",
    "express": "^4.17.2",
    "gulp": "^4.0.2",
    "jest": "^27.4.5",
    "lint-staged": "^12.1.4",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4",
    "ts-node": "^10.4.0",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "prettier": "^2.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-config-prettier": "^8.3.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.ts"
    ],
    "testTimeout": 999999999,
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ]
  }
}
